<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>${basedir}/../project.xml</extend>
  <pomVersion>1</pomVersion>
  <artifactId>cas-server</artifactId>
  <name>JA-SIG Central Authentication Service</name>
  <groupId>cas</groupId>
  <siteDirectory></siteDirectory>
  <repository>
    <connection>scm:cvs:pserver:anonymous@uportal.org:/home/cvs/jasig:cas</connection>
    <developerConnection>scm:cvs:pserver:${maven.username}@uportal.org:/home/cvs/jasig:cas</developerConnection>
    <url>http://www.uportal.org/cgi-bin/viewcvs.cgi/cas3/core/</url>
  </repository>
  <build>
    <resources>
      <resource>
        <directory>${basedir}/../</directory>
        <targetPath>META-INF</targetPath>
        <includes>
          <include>notice.txt</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
</project>

