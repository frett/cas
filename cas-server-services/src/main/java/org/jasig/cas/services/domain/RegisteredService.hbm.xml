<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jasig.cas.services.domain">
	<class name="MutableRegisteredServiceImpl" table="services">
		<id column="id" />

		<property name="allowedAttributeNames" not-null="true" />
		<set name="allowedAttributeNames" table="attributes">
			<key column="service_id" />
			<one-to-many class="MutableRegisteredServiceImpl" />

		</set>

		<property name="allowedToProxy" not-null="true" />

		<property name="enabled" not-null="true" />

		<property name="name" not-null="true" />

		<property name="ssoParticipant" not-null="true" />

		<property name="url" not-null="true" />
	</class>
</hibernate-mapping>