<cruisecontrol>
	<system>
		<configuration>
			<threads count="1" />
		</configuration>
	</system>
	<project
		name="cas3"
		buildafterfailed="true">
		<listeners>
			<currentbuildstatuslistener file="status.html" />
		</listeners>
		<bootstrappers>
			<cvsbootstrapper
				localWorkingCopy="/jasigdev/cruisecontrol/workspace/cas3/"
				cvsroot=":pserver:anonymous@uportal.org:/home/cvs/jasig" />
		</bootstrappers>
		<modificationset
			requiresmodification="false"
			quietperiod="60">
			<alwaysbuild
				username="cruisecontrol" />
		</modificationset>
		<schedule>
			<maven
				mavenscript=""
				projectfile="/jasigdev/cruisecontrol/workspace/cas3/project.xml"
				goal="cas:release"
				time="1400" />
		</schedule>
		<log
			dir="/jasigdev/cruisecontrol/workspace/logs/cas3">
		</log>
	</project>
</cruisecontrol>